<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="testng-mockito-log4j" parallel="methods">
    <listeners>
        <listener class-name="testessentials.mockito.testng.listener.FixtureListener"/>
        <listener class-name="testessentials.mockito.testng.listener.AutoCloseableListener"/>
        <listener class-name="testessentials.mockito.testng.listener.StaticMockListener"/>
        <listener class-name="testessentials.mockito.testng.listener.UnitUnderTestListener"/>
        <listener class-name="testessentials.mockito.log4j.testng.MockitoLog4jListener"/>
    </listeners>
    <test name="unittests">
        <packages>
            <package name="testessentials.testng.mockito.log4j"/>
        </packages>
    </test>
</suite>
